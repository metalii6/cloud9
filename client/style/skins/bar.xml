<a:skin xmlns:a="http://ajax.org/2005/aml">
<a:bar name="bar">
    <a:style><![CDATA[
        .bar{
            overflow : hidden;
            position : relative;
        }
    ]]></a:style>
    <a:presentation>
        <a:main container="div">
            <div class="bar">
                <div class="contents"> </div>
                <div class="barFooter"><div><div> </div></div></div>
            </div>
        </a:main>
    </a:presentation>
</a:bar>
<a:bar name="basic">
    <a:style><![CDATA[
        .basic{
            position : relative;
        }
    ]]></a:style>

    <a:presentation>
        <a:main container=".">
            <div class="basic">
            </div>
        </a:main>
    </a:presentation>
</a:bar>
    <a:bar name="c9-menu-bar">
    <a:style><![CDATA[
        .c9-menu-bar {
            position      : relative;
            overflow      : visible;
            height        : 36px;
            background    : url(images/c9-menu-backg.png) repeat-x 0 0;
            border-bottom : 1px solid #000000;
        }
        
        .c9-menu-bar .c9-mbar-logo {
            position   : relative;
            overflow   : visible;
            height     : 36px;
            background : url(images/cloud9logo.png) no-repeat right 0;
        }
        
        .c9-menu-bar .c9-mbar-logo .c9-mbar-bcont {
            position   : relative;
            overflow   : visible;
            height     : 36px;
            margin-right:200px;
        }
        
        .c9-menu-bar .c9-mbar-logo .c9-mbar-bcont .c9-mbar-round {
            position:absolute;
            overflow:hidden;
            top:0;
            right:0;
            background:url(images/c9-menu-round.png) no-repeat 0 0;
            width:42px;
            height:34px;
        }
        
        .c9-menu-bar .c9-mbar-logo .c9-mbar-bcont .c9-offline {
            width:42px;
            height:21px;
            position:absolute;
            background:url(images/offline.png) no-repeat 0 0;
            top:23px;
            right:-66px;
            display:none;
        }
        
        .c9-menu-bar.offline .c9-mbar-logo .c9-mbar-bcont .c9-offline {
            display:block;
        }
        
        .c9-menu-bar .c9-mbar-logo .c9-mbar-bcont .c9-mbar-cont {
            position:relative;
            overflow:visible;
            margin:0 42px 0 0;
            height:34px;
            background:url(images/c9-menu-backg2.png) repeat-x 0 5px;
        }
    ]]></a:style>

    <a:presentation>
        <a:main container="div/div/div[1]">
            <div class="c9-menu-bar"> 
                <div class="c9-mbar-logo"> 
                    <div class="c9-mbar-bcont">
                        <div class="c9-mbar-cont"> </div>
                        <div class="c9-mbar-round"></div>
                        <div class="c9-offline"></div>
                    </div>
                </div>
            </div>
        </a:main>
    </a:presentation>
</a:bar>

<a:bar name="black-menu-bar">
    <a:style><![CDATA[
        .black-menu-bar {
            width            : 65px;
            overflow         : hidden;
            position         : relative;
            background-image : url(images/gitui_menu_backg.png);
        }
     ]]></a:style>

    <a:presentation>
        <a:main container=".">
            <div class="black-menu-bar"> </div>
        </a:main>
    </a:presentation>
</a:bar>
<a:bar name="debug-panel">
    <a:style><![CDATA[
        .debug-panel {
            position         : relative;
            overflow         : hidden;
            width            : 43px;
            border-left      : 1px solid #2e2e2e;
            background-color : #e7e7e7;
            filter           : progid:DXImageTransform.Microsoft.gradient(GradientType=1,startColorstr='#f3f3f3', endColorstr='#e5e5e5');
            -ms-filter       : "progid:DXImageTransform.Microsoft.gradient(GradientType=1,startColorstr='#f3f3f3', endColorstr='#e5e5e5')";
            background       : -webkit-gradient(linear, left center, right center, from(#f3f3f3), color-stop(1, #e5e5e5));
            background       : -moz-linear-gradient(center left, #f3f3f3 0%, #e5e5e5 100%) repeat scroll 0 0 transparent;
        }
        
        .debug-panel .bar_inset {
            width            : 43px;
            height           : 12px;
            position         : relative;
            background       : url(images/sidebar_right_bar_inset.gif);
        }
        
        .debug-panel .body {
            position    : absolute;
            top         : 13px;
            left        : 0;
            right       : 0;
            bottom      : 0;
            border-top  : 1px solid #ffffff;
        }
    ]]></a:style>

    <a:presentation>
        <a:main container="div[2]">
            <div class="debug-panel">
                <div class="bar_inset"/>
                <div class="body"/>
            </div>
        </a:main>
    </a:presentation>
</a:bar>
<a:bar name="dock-panel">
    <a:style><![CDATA[
        .dock-panel {
            position         : relative;
            overflow         : hidden;
            width            : 65px;
            border-right     : 1px solid #2e2e2e;
            background       : url(images/dock_cloud9_bg.gif);
        }

        .dock-panel .body {
            position    : absolute;
            top         : 0;
            left        : 0;
            right       : 0;
            bottom      : 0;
        }
        
        .dock-panel .c9menulogo {
            width    : 65px;
            height   : 32px;
            position : relative;
            margin-top: 20px;
        }
        
        .dock-panel .c9menulogo span {
            background  : url(images/c9menulogo.png) no-repeat 0 0;
            width       : 45px;
            height      : 29px;
            position    : absolute;
            left        : 50%;
            margin-left : -22px;
            top         : 0;
        }
        
        .dock-panel .c9version {
            color       : #696969;
            font-family : Tahoma;
            font-size   : 10px;
            text-align  : center;
            padding     : 0 0 3px 0;
        }
    ]]></a:style>

    <a:presentation>
        <a:main container="div[1]">
            <div class="dock-panel">
                <div class="body"/>
            </div>
        </a:main>
    </a:presentation>
</a:bar></a:skin>