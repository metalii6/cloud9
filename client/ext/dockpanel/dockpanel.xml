<a:application xmlns:a="http://ajax.org/2005/aml">
    <a:bar id="dockPanelRight" skin="debug-panel">
        <a:button group="sidebarRightDock" skin="button-debug" height="0" id="btnSidebarRightDockHidden" class="default_hidden" state="true" />
        <a:button group="sidebarRightDock" skin="button-debug" class="contact_list" state="true" />
        <a:button group="sidebarRightDock" skin="button-debug" class="debug1" state="true" />
        <a:button group="sidebarRightDock" skin="button-debug" class="debug2" state="true" />
        <a:button group="sidebarRightDock" skin="button-debug" class="call_stack" state="true" />
        <a:button group="sidebarRightDock" 
          id      = "todoRB"
          skin    = "button-debug"
          class   = "todo_check"
          state   = "true"
          onclick = "require('c9/ext/todo/todo').toggleWindow(this);"
         />
        <a:divider skin="divider-debugpanel" />
    </a:bar>
</a:application>