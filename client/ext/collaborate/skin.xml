<?xml version='1.0'?>
<a:skin xmlns:a="http://ajax.org/2005/aml">
    <a:list name="collaboratelist">
        <a:style><![CDATA[
            .collaboratelist {
                background: #fff;
                overflow: hidden;
            }
            
            .collaboratelist .menu-item {
               position:relative;
               height:24px;
               line-height: 24px;
               padding: 0px 15px 0px 22px;
               cursor: default;
            }
            
            .collaboratelist .menu-item .icon {
                width:10px;
                height:10px;
                background-color:yellow;
                position:absolute;
                top:7px;
                right:5px;
            }
            
            .collaboratelist .menu-item .status {
                width:8px;
                height:10px;
                background: url(icons/contact_online.png) no-repeat;
                position:absolute;
                top:7px;
                left:7px;
            }
            
            .collaboratelist .offline .status {
                background: url(icons/contact_offline.png) no-repeat;
            }
            
            .collaboratelist .hover {
        		
            }
            
            .collaboratelist .menu-item.selected {
                border-top       : 1px solid #0d76d7;
                filter           : progid:DXImageTransform.Microsoft.gradient(GradientType=0,startColorstr='#5a9edb', endColorstr='#1f79c3');
                -ms-filter       : "progid:DXImageTransform.Microsoft.gradient(GradientType=0,startColorstr='#5a9edb', endColorstr='#1f79c3')";
                background       : -webkit-gradient(linear, left top, left bottom, from(#5a9edb), color-stop(1, #1f79c3));
                background       : -moz-linear-gradient(center top, #5a9edb 0%, #1f79c3 100%) repeat scroll 0 0 transparent;
                color: #fff;
    		}
        ]]></a:style>
    
        <a:presentation>
            <a:main container=".">
                <div class="collaboratelist">
                </div>
            </a:main>
            <a:item
                class   = "."
                caption = "text()"
                select  = "."
            >
                <div class="menu-item">-<div class="status"></div></div>
            </a:item>
            <a:dragindicator>
                <div class='draglist'><span><u>-</u></span></div>
            </a:dragindicator>
            <a:loading>
                <div class="loading">Loading...</div>
            </a:loading>
            <a:empty select="." caption="Loading...">
                <div class="message">Loading users from server...</div>
            </a:empty>
        </a:presentation>
    </a:list>
    <a:scrollbar name="sbcollaborators">
        <a:style><![CDATA[
            .sbcollaborators {
                position            : relative;
                width               : 6px;
                overflow            : hidden;
                padding             : 6px 3px 3px 0;
            }
    
            .sbcollaborators SPAN {
                width               : 6px;
                bottom              : 0;
                height              : 3px;
                position            : absolute;
            }
    
            .sbcollaborators .filler {
                height              : 100%;
                width               : 6px;
                position            : absolute;
                top                 : 0;
                bottom              : 0;
                z-index             : 1;
            }
            .sbcollaborators .mainrail {
                height              : 100%;
                width               : 6px;
                position            : relative;
            }
    
            .sbcollaboratorsDisabled {
                
            }
    
            .sbcollaboratorsDisabled span {
                
            }
    
            .sbcollaboratorsDisabled .filler {
                
            }
    
            .sbcollaborators .indicator {
                height   : 6px;
                display  : none;
                position : absolute;
                overflow : hidden;
                width    : 7px;
                margin   : 0 0 0 0;
                min-height: 20px;
                background: #aaadaf;
                -webkit-border-radius: 3px;
                -moz-border-radius: 3px;
            }
    
            .sbcollaborators .indicator .indi_left {
                height              : 3px;
                width               : 7px;
                position            : absolute;
                top                 : 0;
                left                : 0;
            }
    
            .sbcollaborators .indicator .indi_middle {
                position            : absolute;
                top                 : 3px;
                bottom              : 3px;
                left                : 0;
                width               : 7px;
            }
    
            .sbcollaborators .indicator .indi_right {
                height              : 3px;
                width               : 7px;
                position            : absolute;
                bottom              : 0;
                left                : 0;
            }
    
            .sbcollaboratorsDown .indicator .indi_left {
                
            }
    
            .sbcollaborators .indicator .indi_middle {
                
            }
    
            .sbcollaborators .indicator .indi_right {
                
            }
        ]]></a:style>
        <a:style condition="apf.isWebkit">
            .sbcollaborators{
                display : -webkit-box;
            }
    
            .sbcollaborators span{
                left : 0;
            }
            
            .sbcollaborators .mainrail {
                height : auto;
            }
    
            .sbcollaborators .filler{
                left : 0;
                height : auto;
                top : 0;
                bottom : 0;
            }
        </a:style>
    
        <a:presentation>
            <a:main indicator="div/div">
                <div class='sbcollaborators'>
                    <div class="filler">
                        <div class='indicator'>
                            <div class="indi_left"></div>
                            <div class="indi_middle"></div>
                            <div class="indi_right"></div>
                        </div>
                    </div>
                    <div class="mainrail"></div>
                    <span> </span>
                </div>
            </a:main>
        </a:presentation>
    </a:scrollbar>
    <a:modalwindow name="todopanel">
        <a:style><![CDATA[
            .todopanel {
    
            }
            
            .todopanel .hbox.hboxchatinput {
                filter           : progid:DXImageTransform.Microsoft.gradient(GradientType=0,startColorstr='#4a4a4a', endColorstr='#404040');
                -ms-filter       : "progid:DXImageTransform.Microsoft.gradient(GradientType=0,startColorstr='#4a4a4a', endColorstr='#404040')";
                background       : -webkit-gradient(linear, left top, left bottom, from(#4a4a4a), color-stop(1, #404040));
                background       : -moz-linear-gradient(center top, #4a4a4a 0%, #404040 100%) repeat scroll 0 0 transparent;
            }
            
            .todopanel .hbox.hboxchatinput .ta {
                overflow: auto;
                overflow-x: hidden;
                padding: 4px 5px;
                border: none;
                -webkit-border-radius: 4px;
                -moz-border-radius: 4px;
            }
            
            .todopanel .hbox.hboxchatinput .tb_textbox .sbtb_middle {
                height : 20px;
                border : none;
                -webkit-border-radius: 4px;
                -moz-border-radius: 4px;
            }
            
            .todopanel .hbox.hboxchatinput .tb_textbox .sbtb_middle input {
                height: 20px;
                line-height: 20px;
            }
            
            .chat_history_overview {
                padding-bottom: 8px;
                border-bottom: 1px dashed #dbdbdb;
                margin-bottom: 3px;
            }
            
            .chat_history_overview img {
                float: left;
                padding: 3px 3px 0 0;
            }
            
            .todopanel .hbox .chat_text {
                overflow: hidden;
                word-wrap: break-word;
                padding-right: 7px;
                padding-bottom: 5px;
            }
            
            .todopanel .hbox .chat_text .chat_message_block {
                padding-top: 10px;
            }
            
            .todopanel .hbox .chat_text .chat_message_block.same_user {
                padding-top: 3px;
            }
            
            .chat_message_block .chat_header {
                padding-bottom: 5px;
                font-weight: bold;
            }
            
            .chat_message_block .chat_msg_timestamp {
                float: right;
                color: #ababab;
            }
    
            .chat_message_block .timestamp_divider {
                height: 1px;
                margin-bottom: 2px;
                border-bottom: 2px dashed #dbdbdb;
            }
            
            .chat_message_block .chat_msg_timestamp.cmt_divider {
                position   : relative;
                top        : -10px;
                background : #ccc;
                color      : #fff;
                padding    : 0px 3px;
                -webkit-border-radius : 8px;
                -moz-border-radius    : 8px;
            }
        ]]></a:style>
        <a:presentation>
            <a:main container="." collapsed-height="0"
              min-width="0" min-height="0" class=".">
                <div class="todopanel">
                
                </div>
            </a:main>
        </a:presentation>
    </a:modalwindow>
    <a:modalwindow name="todowin">
        <a:style><![CDATA[
            .todowin_container{
                z-index                         : 101;
                padding                         : 3px;
                -moz-border-radius              : 5px;
                -webkit-border-radius           : 5px;
                -webkit-border-top-right-radius : 0px;
                -moz-border-radius-topright     : 0px;
                background: #333 url('images/dockPanelSpeckledBG.png') repeat;
            }
    
            .todowin{
                position              : relative;
                padding               : 20px 0 0 0;
                border   : 1px solid #8c8c8c;
            }
    
            .todowin .todowinhead{
                height           : 22px;
                line-height      : 22px;
                margin           : -20px 0 0px 0;
                background       : #333;
                background       : -moz-linear-gradient(top, #e3e3e3, #d4d4d4 );
                background-image : -webkit-gradient(linear, 0% 0%, 0% 100%, from(#e3e3e3), to(#d4d4d4));
                color            : #202020;
                border-bottom    : 1px solid #8c8c8c;
                font-family      : Trebuchet MS;
                font-size        : 10pt;
            }
            
            .todowin .todowinhead .title {
                display          : inline-block;
                background       : #f6f6f6;
                background       : -moz-linear-gradient(top, #eee, #f6f6f6 );
                background-image : -webkit-gradient(linear, 0% 0%, 0% 100%, from(#eee), to(#f6f6f6));
                padding          : 0 5px 0 13px;
                height           : 24px;
                line-height      : 24px;
                border-right     : 1px solid #8c8c8c;
            }
            
            .todowin .toolbar.top_border {
                border-top: 1px solid #8c8c8c;
            }
            
            .todowin .toolbar {
                background    : #f6f6f6;
                border-bottom : 1px solid #b2b2b2;
            }
    
            .todowin .toolbar .subbar {
                background    : #f6f6f6;
                border-bottom : 1px solid #b2b2b2;
            }
            
            .todowin .toolbar .subbar .toolbarbutton div{
                color: #fff;
            }
    
            .todowinActive .todowinhead{
                background : #25a8e7 url(images/sectionheadactive.gif);
                color      : white;
            }
            
            .todowin .vbox {
                border-top : 1px solid #505153;
            }
            
            .todowin .dockwin {
                overflow: hidden;
                padding: 0px;
            }
            
            .todowin .todopanel.top_border {
            }
            
            .todowin .dockwin .docktitle {
                display: none;
            }
            
            .todowin .dockwin .dockcontent {
                top: 1px;
            }
            
            .todowin .todowincontainer{
                width      : 100%;
                height     : 100%;
                background : #fff;
            }
    
            .todowinCurrent .todowinhead{
                /*border-bottom : 2px solid #c3c3c3;
                padding-bottom  : 2px;
                z-index         : 10;
                position        : relative;*/
            }
    
            /*.todowin .todowinhead div{
                float      : right;
                width      : 14px;
                height     : 14px;
                margin     : -1px 3px 0 0;
                border     : 1px solid transparent;
                background : no-repeat 2px 2px;
            }
    
            .todowin .todowinhead div.hover{
                border : 1px solid white;
            }
            .todowin .todowinhead .max{
                background-image: url(images/cmswin_max.gif);
            }
            .todowinMax .todowinhead .max{
                background-image: url(images/cmswin_restore.gif);
            }
            .todowin .todowinhead div.down{
                background-color : white;
            }*/
    
            .todowin .todowinhead .close {
                margin-left: 5px;
            }
            
            .todowin .todowinhead .close a {
                display    : inline-block;
                width      : 14px;
                height     : 14px;
                background : url(images/close_tab_btn.png) 0px 0px no-repeat;
            }
            
            .todowin .todowinhead .close a:hover {
                background-position: -14px 0px;
            }
            
            .todowin .todowincontainer .list{
                background: transparent;
                border: none;
            }
            
            .todowin .todowincontainer .dockcontent > div {
                margin-left: 5px;
            }
            
            .todowin .todowincontainer .dockcontent .dropdown {
                left: 5px;
            }
            
            .todowin .todolist {
            }
        ]]></a:style>
        <a:presentation>
            <a:main 
              collapsed-height = "20"
              min-width        = "150"
              min-height       = "80"
              container        = "div[1]/div[2]"
              drag             = "div[1]/div[1]" 
              title            = "div[1]/div[1]/div[1]/text()">
                <div class="todowin_container">
                    <div class="todowin">
                        <div class="todowinhead">
                            <div class="title">-
                                <span class="close">
                                    <a href="#" 
                                       onclick="require('ext/collaborate/collaborate').closeChat(event, this)"></a>
                                </span>
                            </div>
                        </div>
                        <div class="todowincontainer"></div>
                    </div>
                </div>
            </a:main>
            <a:button>
                <div> </div>
            </a:button>
        </a:presentation>
    </a:modalwindow>
</a:skin>
