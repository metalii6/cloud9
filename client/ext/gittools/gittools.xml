<a:application xmlns:a="http://ajax.org/2005/aml">
    <a:tab id="tabGitTools" skin="docktab" visible="false">
        <a:page caption="Git Tools">
            <a:slider
              id = "sliderGitLog"
              min = "0"
              max = "50"
              step = "10"
              slide = "snap"
              markers = "true"
            />
            <a:label
              caption = "{sliderGitLog.value}"
            />
        </a:page>
    </a:tab>
</a:application>